#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wizard/code/web development/new/new/drs/node_modules/is-docker/node_modules:/home/wizard/code/web development/new/new/drs/node_modules:/home/wizard/code/web development/new/new/node_modules:/home/wizard/code/web development/new/node_modules:/home/wizard/code/web development/node_modules:/home/wizard/code/node_modules:/home/wizard/node_modules:/home/node_modules:/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wizard/code/web development/new/new/drs/node_modules/is-docker/node_modules:/home/wizard/code/web development/new/new/drs/node_modules:/home/wizard/code/web development/new/new/node_modules:/home/wizard/code/web development/new/node_modules:/home/wizard/code/web development/node_modules:/home/wizard/code/node_modules:/home/wizard/node_modules:/home/node_modules:/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-docker/cli.js" "$@"
else
  exec node  "$basedir/../is-docker/cli.js" "$@"
fi
