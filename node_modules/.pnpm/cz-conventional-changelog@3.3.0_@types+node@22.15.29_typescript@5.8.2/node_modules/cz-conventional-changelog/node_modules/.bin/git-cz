#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/bin/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/bin/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules:/home/wizard/code/web development/new/new/drs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/bin/git-cz" "$@"
else
  exec node  "$basedir/../../../../../commitizen@4.3.1_@types+node@22.15.29_typescript@5.8.2/node_modules/commitizen/bin/git-cz" "$@"
fi
